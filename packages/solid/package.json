{
  "name": "@intl-schematic/solid",
  "version": "1.0.0-rc.7",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/Raiondesu/intl-schematic.git"
  },
  "main": "dist/index",
  "module": "dist/index",
  "types": "src/index",
  "esnext": "src/index",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "tsc --noEmit -w",
    "prerelease": "npm version prerelease --preid=rc",
    "pre-patch": "npm version prerelease --preid=rc",
    "pre-minor": "npm version preminor --preid=rc",
    "pre-major": "npm version premajor --preid=rc",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "peerDependencies": {
    "intl-schematic": "*",
    "solid-js": "^1.8.6"
  },
  "devDependencies": {
    "tsconfig": "*",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.3.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./index": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./Intl": {
      "import": "./Intl.tsx",
      "types": "./Intl.tsx"
    },
    "./Multiline": {
      "import": "./Multiline.tsx",
      "types": "./Multiline.tsx"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "index": [
        "./src/index.ts"
      ],
      "Intl": [
        "./Intl.tsx"
      ],
      "Multiline": [
        "./Multiline.tsx"
      ]
    }
  },
  "author": {
    "name": "Raiondesu",
    "email": "ai@raiondesu.dev",
    "url": "https://github.com/Raiondesu"
  },
  "keywords": [
    "l10n",
    "i18n",
    "i18next",
    "vue-i18n",
    "intl",
    "json schema",
    "formatting",
    "formatter",
    "localization",
    "intl schematic",
    "solid-js",
    "solid",
    "integration"
  ]
}
