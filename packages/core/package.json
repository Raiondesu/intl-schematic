{
  "name": "intl-schematic",
  "version": "1.0.0-rc.1",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Raiondesu/intl-schematic"
  },
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "src/index",
  "scripts": {
    "build:schema": "node ./generate-schema.js",
    "dev": "tsc --noEmit -w",
    "prerelease": "npm version prerelease --preid=rc",
    "pre-patch": "npm version prerelease --preid=rc",
    "pre-minor": "npm version preminor --preid=rc",
    "pre-major": "npm version premajor --preid=rc",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "devDependencies": {
    "json-schema-to-typescript": "^13.0.1",
    "tsconfig": "*",
    "typescript": "^5.3.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./index": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./plugins": {
      "import": "./dist/plugins.js",
      "require": "./dist/plugins.cjs",
      "types": "./src/plugins.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "index": [
        "./src/index.ts"
      ],
      "plugins": [
        "./src/plugins.ts"
      ]
    }
  }
}
