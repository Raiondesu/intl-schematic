{
  "name": "@intl-schematic/plugin-defaults",
  "version": "1.0.0-rc.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Raiondesu/intl-schematic/packages/plugins/default"
  },
  "main": "dist/index",
  "module": "dist/index",
  "types": "index",
  "type": "module",
  "scripts": {
    "build": "tsup src",
    "dev": "tsc --noEmit -w",
    "commit-build": "(git diff --quiet && git diff --staged --quiet) || (git commit -am \"Update dist\")",
    "preversion": "npm run build && npm run commit-build",
    "prerelease": "npm version prerelease --preid=rc",
    "pre-patch": "npm version prerelease --preid=rc",
    "pre-minor": "npm version preminor --preid=rc",
    "pre-major": "npm version premajor --preid=rc",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "peerDependencies": {
    "intl-schematic": "*"
  },
  "dependencies": {
    "@intl-schematic/plugin-locale": "*",
    "@intl-schematic/plugin-arrays": "*",
    "@intl-schematic/plugin-processors": "*"
  },
  "devDependencies": {
    "tsconfig": "*",
    "typescript": "^5.3.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./index": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "index": [
        "./src/index.ts"
      ]
    }
  }
}