{
  "name": "@intl-schematic/cli",
  "version": "1.0.0-rc.7",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/Raiondesu/intl-schematic.git"
  },
  "bin": {
    "intl-schematic": "dist/index.js"
  },
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "tsc --noEmit -w",
    "prerelease": "npm version prerelease --preid=rc",
    "pre-patch": "npm version prerelease --preid=rc",
    "pre-minor": "npm version preminor --preid=rc",
    "pre-major": "npm version premajor --preid=rc",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "dependencies": {
    "colorette": "^2.0.20"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "tsconfig": "*",
    "typescript": "^5.3.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./index": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./init": {
      "import": "./dist/init.js",
      "require": "./dist/init.cjs",
      "types": "./src/init.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "index": [
        "./src/index.ts"
      ],
      "init": [
        "./src/init.ts"
      ]
    }
  },
  "author": {
    "name": "Raiondesu",
    "email": "ai@raiondesu.dev",
    "url": "https://github.com/Raiondesu"
  },
  "keywords": [
    "l10n",
    "i18n",
    "i18next",
    "vue-i18n",
    "intl",
    "json schema",
    "formatting",
    "formatter",
    "localization",
    "intl schematic",
    "vue.js",
    "vue",
    "integration"
  ]
}
